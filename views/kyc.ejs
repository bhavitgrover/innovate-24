  <script>
    function isMobileDevice() {
      return window.innerWidth <= 800;
    }
    window.onload = () => {
      if (isMobileDevice()) {
        console.log("hello");
        const phone = document.getElementById("phone");
        phone.style.width = "100vw";
        phone.style.height = "100vh";
        phone.style.borderRadius = "0";
        document.getElementById("up-time").style.display = "none";
        document.getElementById("back").style.top = "3vh";
        document.getElementById("heading").style.marginTop = "1vh";
        document.getElementById("down-bar").style.position = "fixed";
        document.getElementById("navbar").style.position = "fixed";
      }
      const errorEl = document.getElementById("error");
      const error = errorEl ? errorEl.innerText : null;

      if (error) {
        console.log("yes");
        var notyf = new Notyf({
          position: {
            x: "center",
            y: "bottom",
          },
        });
        notyf.error(error);
      }

      // preview helpers
      function makePreview(file) {
        const wrapper = document.createElement("div");
        // green dot
        const dot = document.createElement("span");
        dot.className = "dot";
        wrapper.appendChild(dot);
        // filename
        const name = document.createElement("span");
        name.className = "fileName";
        name.textContent = file ? file.name : "No file selected";
        wrapper.appendChild(name);
        return wrapper;
      }

      function attachPreview(inputId, previewId) {
        const input = document.getElementById(inputId);
        const preview = document.getElementById(previewId);
        if (!input || !preview) return;

        function update() {
          // clear existing
          preview.innerHTML = "";
          const file = input.files && input.files[0];
          if (!file) {
            const no = document.createElement("span");
            no.className = "fileName";
            no.textContent = "No file selected";
            preview.appendChild(no);
            preview.setAttribute("aria-hidden", "true");
            return;
          }
          const node = makePreview(file);
          preview.appendChild(node);
          preview.setAttribute("aria-hidden", "false");
        }

        input.addEventListener("change", update);
        // init
        update();
      }

      attachPreview("aadharDocument", "aadharPreview");
      attachPreview("landDocument", "landPreview");
    };
  </script>
