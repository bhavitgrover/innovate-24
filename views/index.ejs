  <script>
    function isMobileDevice() {
      return window.innerWidth <= 800;
    }

    window.onload = () => {
      if (isMobileDevice()) {
        // console.log("hello");
        const phone = document.getElementById("phone");
        const boxes = document.getElementById("boxes");
        phone.style.width = "100vw";
        phone.style.height = "100vh";
        phone.style.borderRadius = "0";
        boxes.style.marginTop = "7vh";
        document.getElementById("up-time").style.display = "none";
      }
      const currentLevel = "<%= user.loyaltyLevel %>";
      var chartColor = "#ef9d22";
      if (currentLevel == "Silver") {
        chartColor = "#777";
      } else if (currentLevel == "Gold") {
        chartColor = "#ffd819";
      } else if (currentLevel == "Platinum") {
        chartColor = "cyan";
      }
      document.getElementById(
        "chartColored"
      ).style.border = `3vh solid ${chartColor}`;
      const userPercentage = Number(
        '<%= (typeof percentage !== "undefined" ? percentage : 0) %>'
      );
      const rotateDeg = -(180 - 180 * (userPercentage / 100));
      document.getElementById(
        "chart"
      ).style.transform = `rotate(${rotateDeg}deg)`;
    };
  </script>
