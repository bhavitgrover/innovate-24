      <h1 class="heading">Welcome, <%= user.fname %>! ðŸ‘‹</h1>

      <div
        id="loyalty"
        class="loyalty"
        onclick="window.location.href = '/level'"
      >
        <div class="chartContainer" id="chart">
          <div class="chartColored" id="chartColored"></div>
          <div class="chartBlank"></div>
        </div>
        <div class="chartHider"></div>
        <p>YOUR CURRENT TIER></p>
        <h1 style="font-size: 5.6vh"><%= user.loyaltyLevel %></h1>
      </div>

      <div id="boxes" class="boxes">
        <div class="box">
          <% if (user.production < 1000) { %>
          <h1><%= user.production %></h1>
          <% } else { %>
          <h1><%= Math.round(user.production / 1000) %>k</h1>
          <% } %>
          <p style="color: #466eff">kg</p>
        </div>
        <div class="box">
          <h1><%= user.points %></h1>
          <p style="color: #dc4b52">points</p>
        </div>
        <div class="box">
          <% if (user.money < 1000) { %>
          <h1><%= user.money %></h1>
          <% } else { %>
          <h1><%= Math.round(user.money / 1000) %>k</h1>
          <% } %>
          <p style="color: #01b764">earned</p>
        </div>
      </div>

