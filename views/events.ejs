<script>
  // File preview helpers - mirror behavior from track.ejs
  (function () {
    function makePreview(file) {
      // container: dot + optional thumbnail + filename
      const wrapper = document.createElement('div');
      wrapper.style.display = 'flex';
      wrapper.style.gap = '0.8vh';
      wrapper.style.alignItems = 'center';

      const dot = document.createElement('span');
      dot.className = 'dot';
      wrapper.appendChild(dot);

      // If it's an image, show a small thumbnail
      if (file && file.type && file.type.startsWith('image/')) {
        try {
          const img = document.createElement('img');
          img.src = URL.createObjectURL(file);
          img.style.width = '4vh';
          img.style.height = '4vh';
          img.style.objectFit = 'cover';
          img.style.borderRadius = '6px';
          img.onload = function () {
            // release object URL after load
            URL.revokeObjectURL(img.src);
          };
          wrapper.appendChild(img);
        } catch (e) {
          // ignore thumbnail errors
        }
      }

      const name = document.createElement('span');
      name.className = 'fileName';
      name.textContent = file ? file.name : 'No file selected';
      wrapper.appendChild(name);

      return wrapper;
    }

    function attachPreview(inputId, previewId) {
      const input = document.getElementById(inputId);
      const preview = document.getElementById(previewId);
      if (!input || !preview) return;

      function update() {
        preview.innerHTML = '';
        const file = input.files && input.files[0];
        if (!file) {
          const no = document.createElement('span');
          no.className = 'fileName';
          no.textContent = 'No file selected';
          preview.appendChild(no);
          preview.setAttribute('aria-hidden', 'true');
          return;
        }
        const node = makePreview(file);
        preview.appendChild(node);
        preview.setAttribute('aria-hidden', 'false');
      }

      input.addEventListener('change', update);
      // initial state
      update();
    }

    // bind on DOMContentLoaded to ensure elements exist
    if (document.readyState === 'loading') {
      document.addEventListener('DOMContentLoaded', function () {
        attachPreview('cropImage', 'cropImagePreview');
      });
    } else {
      attachPreview('cropImage', 'cropImagePreview');
    }
  })();
</script>
